<?xml version="1.0" encoding="UTF-8"?>
<itop_design xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.6">
  <constants>
  </constants>
  <classes>
    <class id="ExternalINC" _delta="define">
      <parent>FunctionalCI</parent>
      <properties>
        <category>searchable,structure</category>
        <abstract>false</abstract>
        <key_type>autoincrement</key_type>
        <db_table>externalinc</db_table>
        <db_key_field>id</db_key_field>
        <db_final_class_field>finalclass</db_final_class_field>
        <naming>
          <format>%1$s</format>
          <attributes>
            <attribute id="name"/>
          </attributes>
        </naming>
        <display_template/>
        <icon/>
        <reconciliation>
          <attributes>
            <attribute id="name"/>
            <attribute id="org_id"/>
            <attribute id="organization_name"/>
          </attributes>
        </reconciliation>
      </properties>
      <fields>
        <field id="external_id" xsi:type="AttributeString">
          <sql>external_id</sql>
          <default_value/>
          <is_null_allowed>false</is_null_allowed>
          <display_style/>
        </field>
        <field id="org_id" xsi:type="AttributeExternalKey">
          <sql>org_id</sql>
          <is_null_allowed>false</is_null_allowed>
          <on_target_delete>DEL_MANUAL</on_target_delete>
          <target_class>Organization</target_class>
          <display_style>select</display_style>
        </field>
        <field id="incindent_id" xsi:type="AttributeExternalKey">
          <sql>incident_id</sql>
          <is_null_allowed>false</is_null_allowed>
          <on_target_delete>DEL_MANUAL</on_target_delete>
          <target_class>Incident</target_class>
          <display_style>select</display_style>
        </field>
      </fields>
      <presentation>
        <details>
          <items>
            <item id="incident_id">
              <rank>1</rank>
            </item>
            <item id="org_id">
              <rank>2</rank>
            </item>
            <item id="external_id">
              <rank>3</rank>
            </item>
          </items>
        </details>
        <search>
          <items>
            <item id="external_id">
              <rank>1</rank>
            </item>
          </items>
        </search>
      </presentation>
     </class>
  </classes>
  <menus>
  </menus>
  <user_rights>
    <groups>
    </groups>
    <profiles>
    </profiles>
  </user_rights>
</itop_design>
