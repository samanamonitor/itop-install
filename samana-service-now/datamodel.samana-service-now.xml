<?xml version="1.0" encoding="UTF-8"?>
<itop_design xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.6">
  <constants>
  </constants>
  <classes>
    <class id="ExternalIncident" _delta="define">
      <parent>FunctionalCI</parent>
      <properties>
        <category>searchable,structure</category>
        <abstract>false</abstract>
        <key_type>autoincrement</key_type>
        <db_table>external_inc</db_table>
        <db_key_field>id</db_key_field>
        <naming>
          <format>%1$s</format>
          <attributes>
            <attribute id="name"/>
          </attributes>
        </naming>
        <display_template/>
        <icon/>
        <reconciliation>
          <attributes>
            <attribute id="name"/>
            <attribute id="org_id"/>
            <attribute id="organization_name"/>
          </attributes>
        </reconciliation>
      </properties>
      <fields>
        <field id="external_id" xsi:type="AttributeString">
          <sql>external_id</sql>
          <default_value/>
          <is_null_allowed>false</is_null_allowed>
          <display_style/>
        </field>
        <field id="incident_id" xsi:type="AttributeExternalKey">
          <sql>incident_id</sql>
          <is_null_allowed>false</is_null_allowed>
          <on_target_delete>DEL_MANUAL</on_target_delete>
          <target_class>Incident</target_class>
          <display_style>select</display_style>
        </field>
      </fields>
      <methods/>
      <presentation>
        <details>
          <items>
            <item id="incident_id">
              <rank>1</rank>
            </item>
            <item id="org_id">
              <rank>2</rank>
            </item>
            <item id="external_id">
              <rank>3</rank>
            </item>
            <item id="name">
              <rank>4</rank>
            </item>
          </items>
        </details>
        <search>
          <items>
            <item id="external_id">
              <rank>1</rank>
            </item>
          </items>
        </search>
      </presentation>
     </class>
    <class id="Model" _delta="must_exist">
      <fields>
        <field id="type" xsi:type="AttributeEnum">
          <values>
            <value id="ExternalIncident" _define="define">External Incident</value>
          </values>
        </field>
      </fields>
    </class>
  </classes>
  <menus>
  </menus>
  <user_rights>
    <groups>
      <group id="external_inc">
        <classes>
          <class id="ExternalIncident" />
          <class id="Incident"/>
        </classes>
      </group>
    </groups>
    <profiles>
      <profile id="60" _delta="define">
        <name>External Incident</name>
        <description>
          Allows access to the External Incident table.
        </description>
        <groups>
          <group id="external_inc">
            <actions>
              <action id="action:write">allow</action>
              <action id="action:bulk write">allow</action>
              <action id="action:read">allow</action>
              <action id="action:bulk read">allow</action>
            </actions>
          </group>
        </groups>
      </profile>
    </profiles>
  </user_rights>
</itop_design>
